# -*- coding: utf-8 -*-
a = "Olá pessoas do "
b = "Brasil"
juntar = a + "meu " + b
print (juntar)
tamanho = len(juntar)
print (tamanho)
print (a[1])
print (a[0:3])
normal = "Olá mundo"
print (normal.lower())
print (normal.upper())
normal = normal + "\n" + "tudo bem?"
print (normal)
variavel = "oi" + "\n"
print (variavel.strip())
